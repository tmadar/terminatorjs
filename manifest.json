{
  "manifest_version": 2,
  "name": "Twitch Blackout!",
  "version": "2.0",
  "description": "My first Chrome extension.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
	{
     		"matches": ["http://www.twitch.tv/*"],
     		"js": ["terminator.js"],
                "run_at": "document_idle"
    	}
   ]
}